{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "FocusBoard",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "/home/anuruprkris/Project/FocusBoard/frontend",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "cd /home/anuruprkris/Project/FocusBoard/frontend && yarn run dev",
    "beforeBuildCommand": "cd /home/anuruprkris/Project/FocusBoard/frontend && yarn run build"
  },
  "app": {
    "windows": [
      {
        "title": "FocusBoard",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
